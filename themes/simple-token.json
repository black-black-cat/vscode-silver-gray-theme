{
  "$schema": "vscode://schemas/color-theme",
  "type": "dark",
  "tokenColors": [
      {
          "name": "Comment",
          "scope": [
              "comment",
              "string.comment",
              "support.n64asm.comment"
          ],
          "settings": {
              "foreground": "#858585",
              "fontStyle": "italic"
          }
      },
      {
          "name": "Function name",
          "scope": [
              "entity.name.function",
              "support.function",
              "function",
              "function.support.builtin",
              "function.support.core",
              "support.n64asm.label"
          ],
          "settings": {
              "foreground": "#00e"
          }
      },
      {
          "name": "Constant",
          "scope": [
              "constant.language",
              "constant.character",
              "constant.other",
              "variable.language",
              "support.constant",
              "support.n64asm.constant"
          ],
          "settings": {
              "foreground": "#080"
          }
      },
      {
          "name": "Number",
          "scope": [
              "constant.numeric",
              "support.n64asm.number"
          ],
          "settings": {
              "foreground": "#750"
          }
      },
      {
          "name": "String",
          "scope": [
              "string",
              "string.template"
          ],
          "settings": {
              "foreground": "#D000D0"
          }
      },
      {
          "name": "Keyword",
          "scope": [
              "keyword",
              "keyword.operator",
              "keyword.operator.logical",
              "keyword.operator.constructor",
              "entity.name.tag",
              "support.dictionary.json",
              "support.n64asm.instructions",
              "storage.type",
              "storage"
          ],
          "settings": {
              "foreground": "#80a",
              "fontStyle": "bold"
          }
      }
  ]
}